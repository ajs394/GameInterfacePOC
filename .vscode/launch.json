{
    "version": "0.0.1",
    "configurations": [
        {
            "name": "Python",
            "type": "python",
            "pythonPath":"${config.python.pythonPath}", 
            "request": "launch",
            "stopOnEntry": false,
            "console": "none",
            "program": "${workspaceRoot}/__init__.py",
            "cwd": "${workspaceRoot}",
            "debugOptions": [
                "WaitOnAbnormalExit",
                "WaitOnNormalExit",
                "RedirectOutput"
            ]
        }
    ]
}